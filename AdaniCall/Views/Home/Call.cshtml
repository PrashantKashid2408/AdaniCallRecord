<!doctype html>
<html>
<head>
    <meta charset="utf-8" />
    <!-- for Internet IE Explore new elements are render like HTML 5. -->
    <meta http-equiv="X-UA-Compatible" content="IE=edge, chrome=1">
    <meta content='width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no' name='viewport'>

    <meta http-equiv="X-UA-Compatible" content="IE=edge, chrome=1">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <meta name="mobile-web-app-capable" content="yes">
    <meta content="black" name="apple-mobile-web-app-status-bar-style">
    <meta content="YES" name="apple-touch-fullscreen">

    <!-- for apple mobile capable -->
    <meta content="black" name="apple-mobile-web-app-status-bar-style">
    <meta content="yes" name="apple-mobile-web-app-capable">
    <meta content="yes" name="mobile-web-app-capable">
    <meta content="yes" name="apple-touch-fullscreen">

    <title>@ViewBag.Title</title>
    <link rel="shortcut icon" href="~/images/favicon.ico">
    <link href="~/css/style.css?v=9" rel="stylesheet">

    <script src="~/js/jquery-3.6.1.min.js"></script>
    <script src="~/Scripts/common/global.js"></script>
    <script src="~/Scripts/common/common.js?v=1"></script>
    <script src="~/js/common.js?v=10"></script>
    <script src="~/js/camvas.js"></script>
    <script src="~/js/pico.js"></script>
    <script src="~/js/index.js"></script>
   @* <script>
        $(document).ready(function () {
            // for disabled right click
            $(this).bind("contextmenu", function (e) {
                e.preventDefault();
            });
            // for disabled browser back
            window.history.pushState(null, "", window.location.href);
            window.onpopstate = function () {
                window.history.pushState(null, "", window.location.href);
            };

        });
    </script>*@
</head>

<body style="background: url(../images/container-bg.png) no-repeat; background-position: center; background-size: cover;">
    <div class="mainContainer" style="padding-top: 200px; padding: 200px 0px 0px 0px;">
        <div class="main_header" style="visibility: hidden;">
            <div class="textRight"> <img src="~/images/logos/adani_airport_logo.png" class="logo_top" alt="adani airports"> </div>
            <div class="clearfix"></div>
        </div>
        <div class="clearfix"></div>
        <div class="video_container_holder">
            <div class="video_container">
                <div class="video_container_inner">
                    <div id="remoteVideoContainer" class="videoContainerCall " style="width:100%;" hidden></div>
                    <div class="all_steps video_container_loader" id="allStepID">
                        <div class="all_steps_inner">

                        </div>
                        <div class="clearfix"></div>
                    </div>
                    <canvas id="cnFaceDetect" class="cnFaceDetect" width="788" height="590" style="display:none;"></canvas>
                    <div class="clearfix"></div>
                </div>
                <input id="user-access-token"
                       type="text"
                       value="@ViewBag.CallToken"
                       placeholder="User access token"
                       style="margin-bottom:1em; width: 500px;display:none" />
                <button id="initialize-call-agent" type="button" style="display:none">Initialize Call Agent</button>
                <input type="hidden" id="hdnCallTimeOut" value="" />
                <input type="hidden" id="hdnCallStatus" value="" />
                <input id="callee-acs-user-id"
                       type="text"
                       value=""
                       placeholder="Enter callee's ACS user identity in format: '8:acs:resourceId_userId'"
                       style="margin-bottom:1em; width: 500px; display: none;" />
                <div class="video_button_container" @*style="display:none;"*@>
                    <ul class="btnlist">
                        <li>
                            <span>
                                <button id="start-call-button" type="button" disabled="true" class="btn1  btn-disable" @*style="display:none;"*@><img src="~/images/button-icon/startcall_ic.svg" alt="startcall_ic.svg" /> Start call</button>
                            </span>
                        </li>
                        <li>
                            <span>
                                <button id="hangup-call-button" type="button" class="btn2" style="display:none;"><img src="~/images/button-icon/hangcall_ic.svg" alt="hangcall_ic.svg" /> Hang up</button>
                                <button id="hdnhangup-call-button" type="button" style="visibility:hidden;height:0px; padding: 0px; border: 0px; line-height: 0px; position: absolute;"></button>

                            </span>
                        </li>
                        <li>
                            <span>
                                <button id="accept-call-button" type="button" class="btn3" style="display:none"><img src="~/images/button-icon/acceptcall_ic.svg" alt="acceptcall_ic.svg" /> Accept</button>
                            </span>
                        </li>
                        <li>
                            <span>
                                <button id="start-video-button" type="button" class="btn4" style="display:none"><img src="~/images/button-icon/start_video_ic.svg" alt="start_video_ic.svg" /> Start video</button>
                            </span>
                        </li>
                        <li>
                            <span>
                                <button id="stop-video-button" type="button" class="btn5" style="display:none"><img src="~/images/button-icon/stopvideo_ic.svg" alt="stopvideo_ic.svg" /> Stop video</button>
                            </span>
                        </li>
                    </ul>
                    <div class="clearfix"></div>
                </div>
                <div id="connectedLabel" style="color: #13bb13;" hidden>Call is connected!</div>
                <div id="localVideoContainer" style="width: 30%;" hidden>Local video stream:</div>
                <!-- points to the bundle generated from client.js -->
                <script src="~/bundle.js?v=7"></script>
                <div class="clearfix"></div>
            </div>
            <div class="clearfix"></div>
        </div>
        <div class="clearfix"></div>
        <div class="main_footer" style="visibility: hidden;">
            <div class="custom_inner_footer">
                <div class="fLeft"><img src="~/images/logos/microsoft_azure_logo.png" class="microsoft_logo" alt="Microsoft Azure"></div>
                <div class="fRight"> @*<a href="https://localhost:44391/TermsAndConditions" class="links footerLink" target="_blank" rel="noopener">Terms And Conditions</a> &nbsp;|&nbsp;*@ <span class="poweredbytext">Powered by</span> <img src="~/images/logos/gophygital_logo.png" class="gophygital_logo" alt="Gophygital"> </div>
                <div class="clearfix"></div>
            </div>
            <div class="clearfix"></div>
        </div>
        <div class="clearfix"></div>
        <div class="main_header1">
            <div class="textLeft" id="dvLog" style="display:none"></div>
            <div class="clearfix"></div>
        </div>
        <div class="clearfix">&nbsp;</div>
    </div>
    <!-- jQuery -->
    @*<script src="~/Scripts/jquery-2.1.3.js"></script>*@
    <!-- Bootstrap 4 -->
    @*<script src="~/Scripts/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>*@
    <!-- AdminLTE App -->
    @*<script src="~/Scripts/dist/js/adminlte.min.js"></script>*@
    <script id="scriptLoader" type="application/htmlMessage">
        <div class="overlay"></div>
        <div class="loading-img"></div>
    </script>
    <script id="scriptLoaderSmall" type="application/htmlMessage">
        <div class="overlay"></div>
        <div class="loading-img-small"></div>
    </script>
    <script id="scriptAlert" type="application/htmlMessage">
        <div class="alert alert-[[CLASS]] alert-dismissable" style="margin-left: 0px;">
            <!--<i class="fa fa-[[ICON]]"></i>-->
            <button aria-hidden="true" data-dismiss="alert" class="close" type="button">×</button>
            [[MESSAGE]]
        </div>
    </script>
    <script>
        $("#initialize-call-agent").click();
    </script>
</body>
</html>
